{
  "name": "jump-search",
  "displayName": "JumpSearch",
  "description": "Search. Jump. Navigate your codebase effortlessly with history support.",
  "publisher": "srinivas-batthula",
  "version": "1.4.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other",
    "Education"
  ],
  "keywords": [
    "search",
    "jump",
    "navigation",
    "history",
    "regex"
  ],
  "icon": "src/assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/srinivas-batthula/VSCode-Extension"
  },
  "contributes": {
    "commands": [
      {
        "command": "jumpSearchExtension.helloWorld",
        "title": "Intro Starter Panel : JumpSearch"
      },
      {
        "command": "jumpSearchExtension.searchAndJump",
        "title": "Search & Navigate to Text in Workspace : JumpSearch"
      },
      {
        "command": "jumpSearchExtension.showSearchHistory",
        "title": "Show Search History : JumpSearch"
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "release:patch": "npm version patch -m \"ðŸ”– Release %s\" && git push && git push --tags",
    "release:minor": "npm version minor -m \"ðŸ”– Release %s\" && git push && git push --tags",
    "release:major": "npm version major -m \"ðŸ”– Release %s\" && git push && git push --tags"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  }
}
